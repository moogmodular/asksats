{
    "name": "asksats",
    "version": "0.0.1",
    "private": true,
    "license": "MIT",
    "scripts": {
        "build:1-migrate": "prisma migrate deploy",
        "build:2-build": "next build",
        "build": "run-s build:*",
        "db-seed": "prisma db seed",
        "db-migrate-dev": "yarn prisma migrate dev",
        "db-reset": "yarn prisma migrate reset",
        "dev": "next dev",
        "dx": "run-s db-migrate-dev db-seed dev",
        "start": "next start",
        "lint": "eslint src",
        "lint-fix": "yarn lint --fix",
        "test-dev": "start-server-and-test dev 3000 test",
        "test-start": "start-server-and-test start 3000 test",
        "test": "run-s test:*",
        "test:e2e": "playwright test",
        "dev:e2e": "concurrently --kill-others \"yarn dev\" \"playwright test\"",
        "e2e:seed": "prisma db seed",
        "e2e:dev": "yarn dev",
        "e2e:test": "playwright test ./playwright/transaction.test.ts",
        "e2e": "run-p e2e:seed e2e:dev e2e:test",
        "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
        "test:unit": "vitest run",
        "postinstall": "prisma generate"
    },
    "prisma": {
        "seed": "yarn ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "prettier": {
        "printWidth": 120,
        "singleQuote": true,
        "useTabs": false,
        "tabWidth": 4,
        "semi": false,
        "bracketSpacing": true,
        "trailingComma": "all"
    },
